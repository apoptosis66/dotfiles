theme[available_end]="{{color_06}}"

# Mem/Disk used meter
theme[used_start]="{{color_04}}"
theme[used_mid]="{{color_05}}"
theme[used_end]="{{color_06}}"

# Download graph colors
theme[download_start]="{{color_04}}"
theme[download_mid]="{{color_05}}"
theme[download_end]="{{color_06}}"

# Upload graph colors
